# https://github.com/browserslist/browserslist
# https://browserslist.dev/?q=bGFzdCAyIHZlcnNpb25zIGFuZCBub3QgZGVhZCBhbmQgPiAwLjAxJSBvciA%2BIDAuMiU

[all]
last 2 versions and not dead and > 0.01% or > 0.2%

[modern]
last 2 versions and not dead and > 0.01% or > 0.2% and supports es6-module

[legacy]
last 2 versions and not dead and > 0.01% or > 0.2% and not supports es6-module

[all-exclude-opera-mini]
last 2 versions and not dead and > 0.01% or > 0.2% and not op_mini all
