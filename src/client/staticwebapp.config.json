{
  "platform": {
    "apiRuntime": "dotnet:6.0"
  },
  "routes": [
    {
      "route": "/",
      "headers": {
        "X-Frame-Options": "DENY",
        "Permissions-Policy": "fullscreen=(self)"
      }
    },
    {
      "route": "/*.{mjs,js,css,png,svg,jpg,jpeg,gif,ico}",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable",
        "ETag": ""
      }
    }
  ]
}
